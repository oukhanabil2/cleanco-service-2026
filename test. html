        console.log('DataManager:', DataManager);
    }
    
    function testMenu() {
        const menu = MenuManager ? '✅ OK' : '❌ Erreur';
        document.getElementById('result').innerHTML = 
            `MenuManager: ${menu}`;
        console.log('MenuManager:', MenuManager);
    }
